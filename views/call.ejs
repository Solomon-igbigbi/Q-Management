						<div class="box-body">
							<form action="/call" method="POST" role="form">
								<!-- select -->
								<div class="form-group">
									<label>DEPARTMRENT</label>
									<select class="form-control" name="name">
										<option disabled>Select Department</option>
										<% for (var dept of depts) { %>
											<option><%= dept.name %></option>
										<% } %>
									</select>
								</div>

								<!-- Select multiple-->
								<div class="form-group">
									<label>COUNTER</label>
									<select class="form-control" name="counter">
										<option disabled>Select Department</option>
										<% for (var count of counts) { %>
											<option><%= count.name %></option>
										<% } %>
									</select>
								</div>					
								<!-- /.box-body -->
								<div class="box-footer">
									<button type="submit" class="btn btn-info pull-right">CALL NEXT  <i class="fa fa-arrow-circle-right"></i> </button>
								</div>
								<!-- /.box-footer -->
							</div>
							</form>
						</div>
